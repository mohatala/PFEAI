<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff747674" android:angle="270" android:centerY="0.5" android:centerColor="#ff5a5d5a"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#ff8c00" android:endColor="@color/OrangeRed" android:angle="0" android:centerColor="@color/greenish"/>
            </shape>
        </clip>
    </item>
</layer-list>
